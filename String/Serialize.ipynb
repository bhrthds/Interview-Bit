      "source": [
        "class Solution:\n",
        "    # @param A : list of strings\n",
        "    # @return a strings\n",
        "    def serialize(self, A):\n",
        "        s = \"\"\n",
        "        for i in range(len(A)):\n",
        "            s = s + str(A[i])+str(len(A[i]))+\"~\"\n",
        "        return s"
      ]
    }
  ]
