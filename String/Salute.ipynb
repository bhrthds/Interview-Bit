        "class Solution:\n",
        "    # @param A : string\n",
        "     # @return an long\n",
        "    def countSalutes(self, A):\n",
        "        s = 0\n",
        "        s2 = 0\n",
        "        for i in range(len(A)):\n",
        "            if A[i]==\"<\":\n",
        "                s = s+(i-s2)\n",
        "                s2 = s2+1\n",
        "        return s"
      ]
    }
  ]
