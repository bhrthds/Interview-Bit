      "source": [
        "from collections import Counter\n",
        "class Solution:\n",
        "    # @param A : list of integers\n",
        "    # @return a list of integers\n",
        "    def solve(self, A):\n",
        "        a = Counter(A)\n",
        "        a0 = a[0]\n",
        "        a1 = a[1]\n",
        "        return [0]*a0+[1]*a1\n"
      ]
    }
  ]
}